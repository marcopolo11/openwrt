
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'HT40'
	option country '00'
	option distance '30'
	option txpower '17'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'lede'
	option encryption 'psk-mixed+tkip+ccmp'
	option key 'password'
