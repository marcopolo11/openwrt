config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/10180000.wmac'
	option country '00'
	option channel '1'
	option htmode 'HT40'
	option txpower '17'
	option distance '30'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'lede'
	option encryption 'psk-mixed+tkip+ccmp'
	option key 'password'
